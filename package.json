{
  "name": "@pawan-pk/react-native-mapbox-navigation",
  "version": "0.5.2",
  "description": "Mapbox React Native SDKs enable interactive maps and real-time, traffic-aware turn-by-turn navigation, dynamically adjusting routes to avoid congestion.",
  "main": "./src/index.tsx",
  "files": [
    "src",
    "android",
    "ios",
    "cpp",
    "support",
    "*.podspec",
    "!docs",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\""
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "mapbox",
    "carplay",
    "navigation",
    "react-native-mapbox-navigation",
    "carplay-mapbox-navigation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pawan-pk/react-native-mapbox-navigation.git"
  },
  "author": "Pawan Kumar Kushwaha <pkk4499@gmail.com> (https://github.com/pawan-pk)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pawan-pk/react-native-mapbox-navigation/issues"
  },
  "homepage": "https://github.com/pawan-pk/react-native-mapbox-navigation#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@evilmartians/lefthook": "^1.5.0",
    "@react-native/eslint-config": "^0.73.1",
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.44",
    "commitlint": "^17.0.2",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "18.2.0",
    "react-native": "0.74.3",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native",
      "prettier"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "codegenConfig": {
    "name": "RNMapboxNavigationViewSpec",
    "type": "components",
    "jsSrcsDir": "src"
  }
}
